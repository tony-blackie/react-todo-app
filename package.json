{
  "name": "webpack-react-redux",
  "version": "1.0.0",
  "description": "Test application",
  "main": "src/app.js",
  "scripts": {
    "preinstall": "npm i webpack less mocha webpack-dev-server eslint -g",
    "start": "npm run production",
    "production": "npm run lint && npm test && webpack-dev-server --config webpack.production.config.js",
    "development": "npm run lint && npm test && webpack-dev-server",
    "lint": "eslint src tests",
    "test": "mocha tests"
  },
  "author": "Artem Droganov",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "6.4.0",
    "babel-core": "6.11.4",
    "babel-eslint": "7.1.0",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-class-properties": "6.11.5",
    "babel-plugin-transform-es2015-modules-commonjs": "6.14.0",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "clean-webpack-plugin": "0.1.10",
    "css-loader": "0.23.1",
    "eslint": "3.9.1",
    "eslint-plugin-react": "^6.5.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "mocha": "3.1.2",
    "postcss-loader": "^1.1.0",
    "svgo-loader": "1.1.0",
    "webpack": "1.13.1",
    "webpack-hot-middleware": "^2.13.1"
  },
  "dependencies": {
    "normalize.css": "5.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0"
  },
  "config": {
    "development": {
      "server": {
        "port": 3000
      },
      "entry": "./src/index.js",
      "htmlEntry": "./src/index.html",
      "publicPath": "/",
      "distDir": "./dist",
      "cleanDirs": [],
      "js": {
        "appBundle": "js/app.bundle.js?v=[hash]",
        "vendorBundle": "js/vendor.bundle.js?v=[hash]"
      },
      "css": {
        "appBundle": "css/app.css?v=[hash]",
        "vendorBundle": "css/vendor.bundle.css?v=[hash]"
      },
      "statics": {
        "fonts": "fonts/[name].[ext]",
        "images": "images/[name].[ext]?v=[hash]"
      }
    },
    "production": {
      "server": {
        "port": 3000
      },
      "entry": "./src/index.js",
      "htmlEntry": "./src/index.html",
      "publicPath": "/",
      "distDir": "./dist",
      "cleanDirs": [],
      "js": {
        "appBundle": "js/app.bundle.js?v=[hash]",
        "vendorBundle": "js/vendor.bundle.js?v=[hash]"
      },
      "css": {
        "appBundle": "css/app.css?v=[hash]",
        "vendorBundle": "css/vendor.bundle.css?v=[hash]"
      },
      "statics": {
        "fonts": "fonts/[name].[ext]",
        "images": "images/[name].[ext]?v=[hash]"
      }
    }
  }
}
